*timer script
scrp 2 4 38060 9
	lock
	rnge 30
	anim [ 0 1 2 3 2 1 0 ]
	
	setv va00 posx
	setv va01 posy
	subv va00 5
	addv va01 20

	setv va66 0

	doif ov00 = 0

		esee 2 11 7
			addv va66 1
		next
		targ ownr
		doif va66 le 3
			reps rand 1 3
		*	dbg: outs "Peaking Pie Made"
				new: simp 2 11 7 "ds empathic vendor" 3 42 425
				attr 195
				bhvr 48
				perm 64
				elas 30
				accg 5
				fric 100
				mvto va00 va01
				velo rand -50 50 rand 40 50
				emit 8 .5
				tick 120
			repe
		endi

	elif ov00 = 4	
	
		esee 2 8 6
			addv va66 1
		next
		targ ownr
		doif va66 le 3
			reps rand 1 3
		*	dbg: outs "Yarn Fruit"
				new: simp 2 8 6 "ds empathic vendor" 4 37 426
				attr 195
				bhvr 48
				perm 64
				elas 30
				accg 5
				fric 100
				mvto va00 va01
				velo rand -50 50 rand 40 50
				emit 6 .5
				tick 120
			repe
		endi

	elif ov00 = 8	
	
		esee 2 3 15
			addv va66 1
		next
		targ ownr
		doif va66 le 4
			reps rand 2 4
		*	dbg: outs "Star Seed Made"
				new: simp 2 3 15 "ds empathic vendor" 1 41 427
				attr 195
				bhvr 48
				perm 64
				elas 40
				accg 4
				fric 100
				mvto va00 va01
				velo rand -50 50 rand 40 50
				emit 7 .5
				tick 120
			repe	
		endi

	else
	
		esee 2 11 9
			addv va66 1
		next
		targ ownr
		doif va66 le 4
			reps rand 2 4
				new: simp 2 11 9 "carr" 0 0 563
				attr 210
				bhvr 0
				perm 64
				accg 4
				aero 3
				elas 20
				fric 100
				pose 18
				mvto va00 va01
				velo rand -50 50 rand 40 50
				emit 8 0
				tick 1000
		** Which way does it face?
				setv va03 rand 0 1
				doif va03 = 0
					sets name "direction" "left"
					pose 2
				elif va03 = 1
					sets name "direction" "right"
					pose 6
				endi
			repe
		endi

	endi		
endm

