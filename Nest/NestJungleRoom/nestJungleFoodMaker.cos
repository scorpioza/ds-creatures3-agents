	doif va10 = 0
		setv va00 6751
		setv va01 6153
	elif va10 = 4
		setv va00 6797
		setv va01 6153
	elif va10 = 8
		setv va00 6844
		setv va01 6153
	else
		setv va00 6889
		setv va01 6152		
	endi		

	new: simp 2 4 38060 "ettinseedbank" 4 va10 1
	bhvr 11
	attr 4
	tick rand 200 1000	
	mvto va00 va01
	setv ov00 va10
	addv va10 4	
repe


