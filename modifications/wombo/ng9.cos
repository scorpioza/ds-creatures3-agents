subr hunt
*check if touch a bug or bad bug
	seta va16 null
	setv va17 posx 
	setv va18 posy
	setv va21 0
	setv va19 13
	inst
	*get a babytribble
	etch 2 15 31900
		doif ov00 = 60
			velo 0 0
			subv va17 posx
			doif va17 > -15 or va17 < 15
				doif post > va18
					setv va21 1
				else
					setv va21 2
				endi
				seta va16 targ
			endi
		endi
	next
	*better get a bug

	reps 2
	etch 2 va19 0
		velo 0 0
		subv va17 posx
		doif va17 > -15 or va17 < 15
			doif post > va18
				setv va21 1
			else
				setv va21 2
			endi
			seta va16 targ
		endi
	next
	addv va19 1
	repe

	*eat the bug high and low
	doif va21 = 1
		frat 1
		base 0
		anim [11 12 13]
		over
		gsub eatit
		base 0
		anim [12 11 0]
		over
		
	elif va21 = 2
		base 0
		pose 5
		gsub eatit 
		base 0
		pose 0
	elif va21 = 0 and ov00 = 5
	*if there has not been a bug remember this place as a bad location to hunt 
		seta ov99 ov16
	endi
	slow 
	
*if feeding return to baby if hunt was sucessfull if not try find a better place
	doif ov05 = 2  and va21 ne 0
		gsub flyfeed
	endi

retn
