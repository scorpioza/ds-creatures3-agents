subr poscheck

	setv va17 ov17
	setv va18 ov18

	subv va17 posx
	subv va18 posb

	doif va17 < -20 and va17 >= -50
		doif ov10 ne 0
			setv ov10 0
			gsub turnani
			gsub flyani
		endi
		setv ov10 -1
	elif va17 > 20 and va17 <= 50
		doif ov10 ne 0
			setv ov10 0
			gsub turnani
			gsub flyani
		endi
		setv ov10 1
	elif va17 < -50 
		doif ov10 ne -1
			setv ov10 -1
			gsub turnani
			gsub flyani
		endi
	elif va17 > 50
		doif ov10 ne 1
			setv ov10 1
			gsub turnani
			gsub flyani
		endi
	else
		doif ov10 ne 0
			setv ov10 0
			gsub flyani
		endi
		velo 0 0
		setv va22 1
	endi
	doif va18 < -20
		setv ov11 -1
		setv va22 0
	elif va18 > 20
		setv ov11 1
		setv va22 0
	else
		setv ov11 0
		velo 0 0
	endi

retn
